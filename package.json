  "author": "Adam Soko≈Çowski <adam.sokolowski@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^25.1.4",
    "jest": "^25.2.4",
    "standard-version": "^7.1.0",
    "ts-jest": "^25.3.0",
    "tslib": "^1.11.1",
    "typescript": "^3.8.3"
  }
