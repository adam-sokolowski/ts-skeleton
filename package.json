{
  "name": "@adso/ts-skeleton",
  "version": "1.0.0",
  "description": "Skeleton for typescript util libraries",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "typings": "./dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn tsc",
    "build:watch": "yarn tsc -w",
